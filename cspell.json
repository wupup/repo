{
  "import": ["@cspell/dict-lorem-ipsum/cspell-ext.json"],
  "version": "0.2",
  "caseSensitive": false,
  "dictionaries": ["custom-dictionary"],
  "dictionaryDefinitions": [
    {
      "name": "custom-dictionary",
      "path": "./.cspell/custom-dictionary.txt",
      "addWords": true
    }
  ],
  "language": "en",
  "words": [],
  "patterns": [],
  "ignoreWords": [],
  "ignorePaths": [
    "**/node_modules/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/lib/**",
    "**/docs/**",
    "**/vendor/**",
    "**/public/**",
    "**/static/**",
    "**/out/**",
    "**/tmp/**",
    "**/*.d.ts",
    "**/*.md",
    "**/package.json",
    "**/package-lock.json",
    "**/yarn.lock",
    "**/pnpm-lock.yaml",
    "cspell.json",
    ".cspell",
    "./libs/**"
  ]
}
