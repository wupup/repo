{
  "name": "wupup",
  "version": "1.0.0",
  "private": true,
  "description": "My pnpm monorepo project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build.js",
    "dev": "node ./scripts/dev.js",
    "build:all": "pnpm -r run build",
    "dev:app-vue": "pnpm --filter @wupup/app-vue run dev",
    "build:app-vue": "pnpm --filter @wupup/app-vue run build",
    "dev:utils": "pnpm --filter @wupup/utils run dev",
    "build:utils": "pnpm --filter @wupup/utils run build",
    "lint:prettier": "prettier --write \"**/*.{js,ts,mjs,cjs,tsx,json,css,scss,less,html,md,vue}\"",
    "lint:eslint": "eslint",
    "lint:spellcheck": "cspell lint \"(packages|apps)/**/*.{js,ts,mjs,cjs,tsx,json,css,scss,less,html,md,vue}\"",
    "test": "vitest",
    "commit": "git-cz",
    "prepare": "husky",
    "precommit": "lint-staged",
    "push": "node ./scripts/push-to-all.js",
    "publish:utils": "pnpm --filter \"@wupup/utils\" publish --no-git-checks"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=22.16.0",
    "npm": ">=10.9.0",
    "pnpm": ">=10.16.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@eslint/js": "^9.37.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "commitizen": "^4.3.1",
    "cspell": "^9.2.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "rollup": "^4.52.4",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.93.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vitest": "^3.2.4",
    "vitest-browser-vue": "^1.1.0",
    "vue": "^3.5.22"
  }
}
